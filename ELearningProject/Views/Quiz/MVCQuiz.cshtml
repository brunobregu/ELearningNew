
@{
    ViewBag.Title = "MVCQuiz";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

@using (Html.BeginForm("MVCQuiz", "Quiz", FormMethod.Post))
{
    <div>
        <fieldset>
            <legend>Informacion rreth quiz</legend>
            <label for="quiz">Testi ka ne total 25 pyetje, ku secila pyetje ka 2 pike me nje total prej 50 pikesh.</label><br />
            <label for="fillestar">Nqs merrni nga 0-20 pike niveli juaj eshte fillestar .</label><br />
            <label for="mesatar">Nqs merrni nga 20-36 pike niveli juaj eshte mesatar .</label><br />
            <label for="avancuar">Nqs merrni nga 36-50 pike niveli juaj eshte i avancuar .</label><br />

        </fieldset> <br />
        <fieldset>
            <legend>Pyetja 1</legend>
            <label for="1">Per cfare qendron MVC ?</label><br />
            @Html.RadioButton("Pyetja1", "Model View Controller") Model View Controller<br />
            @Html.RadioButton("Pyetja1", "Metadat View Controller") Metadat View Controller<br />
            @Html.RadioButton("Pyetja1", "Model View Cut") Model View Cut<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 2</legend>
            <label for="1">Cila sherben per te kthyer tekst te thjeshte ?</label><br />
            @Html.RadioButton("Pyetja2", "Json") Json<br />
            @Html.RadioButton("Pyetja2", "HttpNotFound") HttpNotFound<br />
            @Html.RadioButton("Pyetja2", "Content") Content<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 3</legend>
            <label for="1">Cila eshte sintaksa e sakte per ViewBag ?</label><br />
            @Html.RadioButton("Pyetja3", "ViewBag.Afisho") ViewBag.Afisho<br />
            @Html.RadioButton("Pyetja3", "ViewBag['Afisho']") ViewBag['Afisho']<br />
            @Html.RadioButton("Pyetja3", "ViewBag") ViewBag<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 4</legend>
            <label for="1">Per te shmangur dykumptimesine ne Razor perdorim ?</label><br />
            @Html.RadioButton("Pyetja4", "()") ()<br />
            @Html.RadioButton("Pyetja4", "@@") @@<br />
            @Html.RadioButton("Pyetja4", "te dyja") te dyja<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 5</legend>
            <label for="1">Cili do te jete tipi i pershtatshem per emrin ?</label><br />
            @Html.RadioButton("Pyetja5", "int") int<br />
            @Html.RadioButton("Pyetja5", "DateTime") DateTime<br />
            @Html.RadioButton("Pyetja5", "string") string<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 6</legend>
            <label for="1">Cili shfaq nje input te tipit tekst ?</label><br />
            @Html.RadioButton("Pyetja6", "LabelFor") LabelFor<br />
            @Html.RadioButton("Pyetja6", "TextBoxFor") TextBoxFor<br />
            @Html.RadioButton("Pyetja6", "RadioButtonFor") RadioButtonFor<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 7</legend>
            <label for="1">Cili sherben per te afishuar nje element perzgjidhjeje ?</label><br />
            @Html.RadioButton("Pyetja7", "CheckBoxFor") CheckBoxFor<br />
            @Html.RadioButton("Pyetja7", "TextBoxFor") TextBoxFor<br />
            @Html.RadioButton("Pyetja7", "RadioButtonFor") RadioButtonFor<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 8</legend>
            <label for="1">Cili shfaq nje fushe te fshehur ?</label><br />
            @Html.RadioButton("Pyetja8", "CheckBoxFor") CheckBoxFor<br />
            @Html.RadioButton("Pyetja8", "PasswordFor") PasswordFor<br />
