            @Html.RadioButton("Pyetja14", "Required") Required<br />
            @Html.RadioButton("Pyetja14", "StringLength") StringLength<br />
            @Html.RadioButton("Pyetja14", "RegularExpression") RegularExpression<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 15</legend>
            <label for="1">Per cfare qendron LINQ ?</label><br />
            @Html.RadioButton("Pyetja15", "Language Integrated Query") Language Integrated Query<br />
