        <fieldset>
            <legend>Pyetja 22</legend>
            <label for="1">Cila prej te meposhtemeve nuk eshte nje funksion agregat ?</label><br />
            @Html.RadioButton("Pyetja22", "Count") Count<br />
            @Html.RadioButton("Pyetja22", "Sum") Sum<br />
            @Html.RadioButton("Pyetja22", "Select") Select<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 23</legend>
            <label for="1">Ndertohen klasat e modelit dhe me pas gjenerohen tabelat e databazes ?</label><br />
            @Html.RadioButton("Pyetja23", "Database-First") Database-First<br />
