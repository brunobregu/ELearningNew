            @Html.RadioButton("Pyetja12", ".cshtml") .cshtml<br />
            @Html.RadioButton("Pyetja12", ".view") .view<br />
        </fieldset><br />
        <fieldset>
            <legend>Pyetja 13</legend>
            <label for="1">Atributi qe e bene nje fushe te detyrueshme ?</label><br />
            @Html.RadioButton("Pyetja13", "Required") Required<br />
            @Html.RadioButton("Pyetja13", "StringLength") StringLength<br />
